{
  "version": 2,

  "builds": [{
    "src": "./index.js",
    "use": "@vercel/node"
  }],

  "routes": [{
    "src" : "/(.*)",
    "dest" : "/",
    "methods": ["GET", "POST", "OPTIONS"],
    "headers": {
      "Access-Control-Allow-Origin" : "https://react-reseau.vercel.app",
      "Access-Control-Allow-Headers" : "origin, content-type, accept",
      "Access-Control-Allow-Credentials" : "true"
    }
  }]
}